{
  "name": "space-align-format",
	"displayName": "space-align-format",
	"description": "Align spaces.",
	"icon": "vscode-logo.png",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/matsuyoido/space-align-format_vscode_extension"
	},
	"engines": {
		"vscode": "^1.69.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:matsuyoido.vscode.format.space_align",
		"onCommand:matsuyoido.vscode.format.space_align_group",
		"onCommand:matsuyoido.vscode.format.space_align_center"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"category": "Format",
				"command": "matsuyoido.vscode.format.space_align",
				"title": "Space by columns"
			},
			{
				"category": "Format",
				"command": "matsuyoido.vscode.format.space_align_group",
				"title": "Space by columns group"
			},
			{
				"category": "Format",
				"command": "matsuyoido.vscode.format.space_align_center",
				"title": "Space by center character"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "matsuyoido.vscode.format.space_align",
					"group": "Format"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.69.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.18.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"@vscode/test-electron": "^2.1.5"
	}
}
